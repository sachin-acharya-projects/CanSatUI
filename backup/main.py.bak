    def handle_screenstate(self, MainWindow: QtWidgets.QMainWindow):
        if MainWindow.isMaximized():
            MainWindow.showNormal()
            self.maximize_window.setText(self._translate("MainWindow", "Maximize"))
            return "Normal"
        MainWindow.showMaximized()
        self.maximize_window.setText(self._translate("MainWindow", "Normal"))
        return "FullScreen"
    def setStorageCondition(self):
        if self.database.state:
            self.database.stop()
            self.start_storage.setText(self._translate("MainWindow", "Stop Storage"))
            return
        self.database.start()
        self.start_storage.setText(self._translate("MainWindow", "Start Storage"))
        return
